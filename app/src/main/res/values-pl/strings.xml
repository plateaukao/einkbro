    <string name="setting_title_inverted">Odwrócone kolory</string>
    <string name="setting_summary_inverted">Ta funkcja sprawi, że zawartość stron załaduje się w odwróconej skali szarości. Forma trybu nocnego.</string>

    <string name="setting_title_font">Rozmiar czcionki</string>

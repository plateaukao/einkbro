    <string name="setting_title_inverted">Colores invertidos</string>
    <string name="setting_summary_inverted">Si se comprueba, el contenido se renderizará en un modo de escala gris invertido. Esta es una especie de \"modo nocturno\".</string>

    <string name="setting_title_font">Tamaño de fuente</string>
    <string name="setting_summary_font">Cambiar el tamaño de la fuente para mostrar sitios Web.</string>

    <string name="setting_title_remote">Contenido remoto</string>
    <string name="setting_summary_remote">Habilite el contenido remoto, que no es directamente del sitio web que está visitando.         Deshabilitar esto es más seguro, más rápido y ahorra volumen de datos. Pero no se mostrará más contenido.</string>


    <!-- Browser start -->
    <string name="setting_title_images">Imágenes</string>
    <string name="setting_summary_images">Habilitar imágenes de red. La desactivación podría ser útil para ahorrar datos móviles y para cargar sitios web más rápidamente.</string>
    <string name="setting_title_cookie">Cookies</string>
    <string name="setting_summary_cookie_delete">Si está activado, se borrarán todas las cookies. También las cookies de sitios web de confianza.</string>
    <string name="setting_summary_cookie">Habilitar cookies. Las cookies son archivos de texto pequeños, que se almacenan en el dispositivo. Los sitios web los utilizan, por ejemplo, para almacenar la configuración del usuario (por ejemplo, configuración de búsqueda). Pero también se pueden utilizar para recopilar datos personales de los usuarios. Las cookies de terceros (recopilan datos de usuario para publicidad) siempre están deshabilitadas.</string>
    <string name="setting_title_whitelistCookie">lista blanca Cookie</string>
    <string name="setting_summary_whitelistCookie">Si deshabilita las cookies, puede utilizar esta lista blanca para permitir que los sitios en los que confíe utilice cookies guardada.</string>

    <string name="setting_title_javascript">JavaScript</string>
    <string name="setting_summary_javascript">Activar JavaScript. Muchos sitios web necesitan JavaScript para su funcionalidad (menús, carga y visualización de imágenes,…). Pero también son un riesgo y pueden ser utilizados para ataques.</string>
    <string name="setting_title_whitelistJS">lista blanca Javascript</string>
    <string name="setting_summary_whitelistJS">Si deshabilita JavaScript, puede utilizar esta lista blanca para permitir que los sitios en los que confía pueda utilizar JavaScript.</string>

    <string name="setting_title_adblock">AdBlock</string>
    <string name="setting_summary_adblock">Habilite Adblock para bloquear anuncios molestos. Los sitios también se cargan más rápido cuando están activados.</string>
    <string name="setting_title_whitelist">lista blanca AdBlock</string>
    <string name="setting_summary_whitelist">Permitir que estos sitios muestren anuncios. Algunos sitios no funcionan, cuando Adblock está habilitado.</string>

    <string name="setting_title_location">Ubicación</string>
    <string name="setting_summary_location">Permitir que los sitios web accedan a su ubicación. Algunos sitios web (como Google Maps para la navegación) necesitan esto habilitado.</string>

    <string name="setting_title_history">Historia</string>
    <string name="setting_summary_history">Guarda la historia de los sitios web visitados.</string>



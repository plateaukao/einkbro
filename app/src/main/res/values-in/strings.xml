    <!-- Browser start -->
    <string name="setting_title_images">Gambar</string>
    <string name="setting_summary_images">Aktifkan gambar Jaringan. Menonaktifkan dapat berguna untuk menyimpan data mobile dan memuat
    situs web lebih cepat.</string>
    <string name="setting_title_cookie">Cookies</string>
    <string name="setting_summary_cookie">Aktifkan Cookie. Cookie adalah file teks kecil yang disimpan pada perangkat Anda. Situs web
    gunakan mereka misalnya untuk menyimpan pengaturan pengguna (misalnya pengaturan pencarian). Tapi mereka juga bisa digunakan untuk mengumpulkan pribadi
        data pengguna. Kuki pihak ketiga (mengumpulkan data pengguna untuk iklan) selalu dinonaktifkan.</string>

    <string name="setting_title_javascript">JavaScript</string>
    <string name="setting_summary_javascript">Aktifkan JavaScript. Banyak situs web memerlukan JavaScript untuk fungsinya (menu,
        memuat dan menampilkan gambar, …). Tapi mereka juga berisiko dan bisa digunakan untuk serangan.</string>
    <string name="setting_title_whitelistJS">Daftar Putih JavaScript</string>
    <string name="setting_summary_whitelistJS">Jika Anda menonaktifkan JavaScript, Anda dapat menggunakan daftar putih ini untuk mengizinkan situs Anda
        percaya untuk menggunakan javascript.</string>

    <string name="setting_title_adblock">AdBlock</string>
    <string name="setting_summary_adblock">Aktifkan AdBlock untuk memblokir iklan yang mengganggu. Situs juga memuat lebih cepat saat diaktifkan.</string>
    <string name="setting_title_whitelist">Daftar Putih AdBlock</string>
    <string name="setting_summary_whitelist">Izinkan situs ini menampilkan iklan. Beberapa situs tidak bekerja, bila adblocker diaktifkan.</string>

    <string name="setting_title_location">Lokasi</string>
    <string name="setting_summary_location">Izinkan situs web mengakses lokasi Anda. Beberapa situs web (seperti Google maps untuk navigasi)
        butuh ini diaktifkan.</string>


